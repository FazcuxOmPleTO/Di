local Button = Tab:CreateButton({
   Name = "Vida Infinita ‚õëÔ∏è",
   Callback = function()
local carros = workspace:FindFirstChild("CarrosSpawnados")
if not carros then return end

for _, carro in ipairs(carros:GetChildren()) do
    local status = carro:FindFirstChild("Info")
        and carro.Info:FindFirstChild("Status")

    if status then
        -- Congelar Vida
        local vida = status:FindFirstChild("Vida")
        if vida and vida:IsA("NumberValue") then
            vida.Value = 99999
            vida:GetPropertyChangedSignal("Value"):Connect(function()
                vida.Value = 99999
            end)
        end

        -- Congelar Quebrou
        local quebrou = status:FindFirstChild("Quebrou")
        if quebrou and quebrou:IsA("BoolValue") then
            quebrou.Value = false
            quebrou:GetPropertyChangedSignal("Value"):Connect(function()
                quebrou.Value = false
            end)
        end
    end
end
   end,
})

local Button = Tab:CreateButton({
   Name = "Gasolina Infinita ‚õΩ",
   Callback = function()
local carros = workspace:FindFirstChild("CarrosSpawnados")
if not carros then return end

for _, carro in ipairs(carros:GetChildren()) do
    local gasolina = carro:FindFirstChild("Info")
        and carro.Info:FindFirstChild("Status")
        and carro.Info.Status:FindFirstChild("Gasolina")
    
    if gasolina and gasolina:IsA("NumberValue") then
        gasolina.Value = 99999

        -- Congelar o valor
        gasolina:GetPropertyChangedSignal("Value"):Connect(function()
            gasolina.Value = 99999
        end)
    end
end
   end,
})
local Divider = Tab:CreateDivider()

local Button = Tab:CreateButton({
   Name = "‚ñ∂Ô∏è Iniciar",
   Callback = function()
local vehiclePath
local primaryPart
local playerName = game.Players.LocalPlayer.Name

for _, vehicle in ipairs(workspace.CarrosSpawnados:GetChildren()) do
    local ownerInfoPath = vehicle:FindFirstChild("Info") and vehicle.Info:FindFirstChild("Owner")
    if ownerInfoPath and ownerInfoPath.Value == playerName then
        vehiclePath = vehicle
        break
    end
end

-- üßπ Apagar rodas e √°gua
if vehiclePath then
    -- Remove todas as rodas do ve√≠culo
    local wheelsFolder = vehiclePath:FindFirstChild("Wheels")
    if wheelsFolder then
        for _, wheel in ipairs(wheelsFolder:GetChildren()) do
            wheel:Destroy()
        end
    end

    -- Remove objeto de √°gua
    local aguas = workspace:FindFirstChild("Complementos") and workspace.Complementos:FindFirstChild("Aguas")
    if aguas then
        aguas:Destroy()
    end

    -- Define PrimaryPart
    primaryPart = vehiclePath:FindFirstChild("Body") and vehiclePath.Body:FindFirstChild("Colisao") and vehiclePath.Body.Colisao:FindFirstChild("ColisaoPartNew")
    if primaryPart then
        vehiclePath.PrimaryPart = primaryPart
    end
end

_G.TeleportRunning = true
print("üöó Iniciando sequ√™ncia de teleportes...")

local function moveVehicleManually(destinationCFrame, speed)
    print("üîÑ Movendo ve√≠culo para: ", destinationCFrame.Position)

    for _, part in ipairs(vehiclePath:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Anchored = true
        end
    end

    local correctedCFrame = destinationCFrame + destinationCFrame.UpVector * 5
    local currentCFrame = vehiclePath:GetPrimaryPartCFrame()
    local distance = (correctedCFrame.Position - currentCFrame.Position).Magnitude
    local duration = distance / speed

    local startTime = tick()
    while tick() - startTime < duration and _G.TeleportRunning do
        local alpha = (tick() - startTime) / duration
        local newCFrame = currentCFrame:Lerp(correctedCFrame, alpha)
        vehiclePath:SetPrimaryPartCFrame(newCFrame)
        task.wait(0.016)
    end

    if _G.TeleportRunning then
        vehiclePath:SetPrimaryPartCFrame(correctedCFrame)
    end

    task.wait(1)

    for _, part in ipairs(vehiclePath:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Anchored = false
        end
    end

    print("‚úÖ Chegou ao destino.")
end

local function waitForProximityPrompt(targetCFrame)
    print("üì° Procurando ProximityPrompt...")
    for _, descendant in ipairs(workspace.Construcoes.LocaisRotas:GetDescendants()) do
        if descendant:IsA("ProximityPrompt") then
            local promptPart = descendant.Parent:IsA("BasePart") and descendant.Parent or nil
            if promptPart then
                local distance = (promptPart.Position - targetCFrame.Position).Magnitude
                if distance <= 30 then
                    print("‚ö° Ativando prompt de intera√ß√£o.")
                    fireproximityprompt(descendant, 1)
                    task.wait(0.4)
                end
            end
        end
    end
end

local function pressKeyE()
    print("‚å®Ô∏è Pressionando tecla E.")
    local success, errorMessage = pcall(function()
        local virtualInput = game:GetService("VirtualInputManager")
        virtualInput:SendKeyEvent(true, Enum.KeyCode.E, false, game)
        task.wait(0.1)
        virtualInput:SendKeyEvent(false, Enum.KeyCode.E, false, game)
    end)
end

