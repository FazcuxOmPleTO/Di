coroutine.wrap(function()
    if not vehiclePath or not primaryPart then
        warn("‚ùå Ve√≠culo n√£o identificado!")
        return
    end

    local cooldown = 17

    while _G.TeleportRunning do
        for i, targetCFrame in ipairs(teleportLocations) do
            if not _G.TeleportRunning then return end

            print(string.format("üìç Teleporte %d de %d", i, #teleportLocations))

            local currentCFrame = i == 1 and vehiclePath:GetPrimaryPartCFrame() or teleportLocations[i - 1]
            local distance = (targetCFrame.Position - currentCFrame.Position).Magnitude
            local speedFactor = 4
            local safeSpeed = math.min(distance / cooldown * speedFactor, 300)

            moveVehicleManually(targetCFrame, safeSpeed)
            task.wait(0.5)
            waitForProximityPrompt(targetCFrame)
            task.wait(0.5)

            if i == #teleportLocations then
                pressKeyE()
            end

            if i < #teleportLocations then
                print("‚è≥ Aguardando antes do pr√≥ximo teleporte...")
                local waitStart = tick()
                while tick() - waitStart < 17 and _G.TeleportRunning do
                    task.wait(0.4)
                end
            end
        end

        print("üîÅ Reiniciando sequ√™ncia de teleportes em breve...")
        local waitStart = tick()
        while tick() - waitStart < 17 and _G.TeleportRunning do
            task.wait(0.4)
        end
    end

    print("üõë Sequ√™ncia encerrada.")
end)()
   end,
})

